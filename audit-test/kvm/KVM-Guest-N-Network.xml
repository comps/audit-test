<network>
    <name>{network_name}</name>
    <bridge name="{network_bridge_name}" />
  <forward/>
  <ip address="{network_ip_address}" netmask="{network_ip_netmask}">
    <dhcp>
        <range start="{network_ip_dhcp_range_start}" end="{network_ip_dhcp_range_end}" />
    </dhcp>
  </ip>
</network>
