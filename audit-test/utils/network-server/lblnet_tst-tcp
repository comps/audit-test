# LSPP labeled networking test server
#  listens on IPv6/TCP port 4000 for unlabeled connections
#  listens on IPv4/TCP port 4001 for labeled connections

service lblnet_tst_unlabeled_ipv6
{
	id		= lblnet_tst_unlabeled_ipv6
	type		= UNLISTED
	flags		= REUSE IPv6
	wait		= no
	user		= root
	disable		= no

	instances	= 1

	socket_type	= stream
	protocol	= tcp
	port		= 4000

	server		= /usr/local/eal4_testing/audit-test/utils/network-server/lblnet_tst_server
	server_args	= -i -t 10 -l /var/log/lblnet_tst_server.log -f /var/run/lblnet_tst_server6.pid -vv
}

service lblnet_tst_labeled_ipv4
{
	id		= lblnet_tst_labeled_ipv4
	type		= UNLISTED
	flags		= REUSE IPv4 LABELED
	wait		= no
	user		= root
	disable		= no

	instances	= 1

	socket_type	= stream
	protocol	= tcp
	port		= 4001

	server		= /usr/local/eal4_testing/audit-test/utils/network-server/lblnet_tst_server
	server_args	= -i -t 10 -l /var/log/lblnet_tst_server.log -f /var/run/lblnet_tst_server4.pid -vv
}

service lblnet_tst_cleanup
{
	id		= lblnet_tst_cleanup
	type		= UNLISTED
	flags		= REUSE
	wait		= no
	user		= root
	disable		= no

	instances	= 1

	socket_type	= stream
	protocol	= tcp
	port		= 4009

	server		= /usr/local/eal4_testing/audit-test/utils/network-server/pidfile_kill
	server_args	= /var/run/lblnet_tst_server6.pid /var/run/lblnet_tst_server4.pid
}
