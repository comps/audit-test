# LSPP labeled networking test server
#  listens on IPv6/TCP port 4006 for unlabeled connections
#  listens on IPv4/TCP port 4004 for labeled connections

service lblnet_tst_unlabeled_ipv6
{
	id		= lblnet_tst_unlabeled_ipv6
	type		= UNLISTED
	flags		= REUSE IPv6
	wait		= no
	user		= root
	disable		= no

	instances	= 1

	socket_type	= stream
	protocol	= tcp
	port		= 4006

	server		= /usr/local/eal4_testing/audit-test/utils/network-server/lblnet_tst_server
	server_args	= -i -t 10
}

service lblnet_tst_labeled_ipv4
{
	id		= lblnet_tst_labeled_ipv4
	type		= UNLISTED
	flags		= REUSE IPv4 LABELED
	wait		= no
	user		= root
	disable		= no

	instances	= 1

	socket_type	= stream
	protocol	= tcp
	port		= 4004

	server		= /usr/local/eal4_testing/audit-test/utils/network-server/lblnet_tst_server
	server_args	= -i -t 10
}
