Test3 ()
{
	[ "$IDFLAG" ] && {
		echo "Test3 - Tspi_TPM_GetPubEndorsementKey01"
		return 0
	}

	echo
	echo -n "T3: Checking Public Endorsement Key..."
	$TESTPATH/tpm/Tspi_TPM_GetPubEndorsementKey01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		return 0
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 1
}
