Test11 ()
{
	#
	# ID this function with -id argument
	#
	[ "$IDFLAG" ] && {
		echo "Test11 - Tspi_TPM_Delegate_GetFamily01"
		return 0
	}

	echo
	echo -n "T11: Get handle to family delegation..."
	$TESTPATH/tpm/Tspi_TPM_Delegate_GetFamily01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		return 0
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 1
}
