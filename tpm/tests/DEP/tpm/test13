Test13 ()
{
	#
	# ID this function with -id argument
	#
	[ "$IDFLAG" ] && {
		echo "Test13 - Tspi_TPM_Delegate_ReadTables01"
		return 0
	}

	echo
	echo -n "T13: Read public contents of family and delegate tables..."
	$TESTPATH/tpm/Tspi_TPM_Delegate_ReadTables01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 0
}
