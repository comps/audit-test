Test4 ()
{
	[ "$IDFLAG" ] && {
		echo "Test4 - Tspi_TPM_OwnerGetSRKPubKey01"
		return 0
	}

	echo
	echo -n "T4: Checking Storage Root Key..."
	$TESTPATH/tpm/Tspi_TPM_OwnerGetSRKPubKey01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		return 0
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 1
}
