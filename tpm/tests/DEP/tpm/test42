Test42 ()
{
	#
	# ID this function with -id argument
	#
	[ "$IDFLAG" ] && {
		echo "Test42 - Tspi_TPM_DirWrite01"
		return 0
	}

	echo
	echo -n "T42: Tspi_TPM_DirWrite01..."
	$TESTPATH/tpm/Tspi_TPM_DirWrite01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 0
}
