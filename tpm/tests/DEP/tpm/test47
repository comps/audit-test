Test47 ()
{
	#
	# ID this function with -id argument
	#
	[ "$IDFLAG" ] && {
		echo "Test47 - Tspi_TPM_Quote01"
		return 0
	}

	echo
	echo -n "T47: Tspi_TPM_Quote01..."
	$TESTPATH/tpm/Tspi_TPM_Quote01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		return 0
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 1
}
