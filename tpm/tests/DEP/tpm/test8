Test8 ()
{
	[ "$IDFLAG" ] && {
		echo "Test8 - Tspi_TPM_Delegate_AddFamily01"
		return 0
	}

	echo
	echo -n "T8: Checking Delegate AddFamily..."
	$TESTPATH/tpm/Tspi_TPM_Delegate_AddFamily01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 0
}
