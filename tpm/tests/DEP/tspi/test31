Test31 ()
{
	#
	# ID this function with -id argument
	#
	[ "$IDFLAG" ] && {
		echo "Test31 - Tspi_ChangeAuthAsym01"
		return 0
	}

	echo
	echo -n "T31: Tspi_ChangeAuthAsym01..."
	$TESTPATH/tspi/Tspi_ChangeAuthAsym01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		return 0
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 1
}
