Test15 ()
{
	#
	# ID this function with -id argument
	#
	[ "$IDFLAG" ] && {
		echo "Test15 - Tspi_TPM_GetEvent01"
		return 0
	}

	echo
	echo -n "T15: Get a single PCR Event..."
	$TESTPATH/tpm/Tspi_TPM_GetEvent01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 0
}
