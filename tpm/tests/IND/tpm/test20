Test20 ()
{
	#
	# ID this function with -id argument
	#
	[ "$IDFLAG" ] && {
		echo "Test20 - Tspi_TPM_GetTestResults01"
		return 0
	}

	echo
	echo -n "T20: Read Self-Test Results..."
	$TESTPATH/tpm/Tspi_TPM_GetTestResults01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 0
}
