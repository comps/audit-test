Test23 ()
{
	#
	# ID this function with -id argument
	#
	[ "$IDFLAG" ] && {
		echo "Test23 - Tspi_TPM_PcrReset01"
		return 0
	}

	echo
	echo -n "T23: Test PCR Reset..."
	$TESTPATH/tpm/Tspi_TPM_PcrReset01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		return 0
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 1
}
