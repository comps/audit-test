Test27 ()
{
	#
	# ID this function with -id argument
	#
	[ "$IDFLAG" ] && {
		echo "Test27 - Tspi_GetPolicyObject01"
		return 0
	}

	echo
	echo -n "T27: Read policy object..."
	$TESTPATH/tspi/Tspi_GetPolicyObject01 $VERSION 2>>eout >>sout
	case "$?" in
	0 )
		echo "successful."
		;;
	* )
		echo "Failed($ESTAT)."
		echo "-> Check `pwd`/eout for details"
		;;
	esac

	return 0
}
